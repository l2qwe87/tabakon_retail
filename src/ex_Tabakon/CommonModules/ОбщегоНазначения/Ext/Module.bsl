	результ = "";
	
	мРасширений = РасширенияКонфигурации.Получить();
	
	Для Каждого расширение из мРасширений Цикл
		результ = результ + ?(ЗначениеЗаполнено(результ),Символы.ПС,"")+расширение.Имя+":"+расширение.Версия;
